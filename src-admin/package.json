{
    "name": "iobroker.matter",
    "version": "0.1.13",
    "private": true,
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "*"
    },
    "dependencies": {
        "@foxriver76/iob-component-lib": "^0.1.6",
        "@iobroker/adapter-react-v5": "^7.4.0",
        "@iobroker/dm-gui-components": "~7.4.0",
        "@iobroker/type-detector": "^4.0.1",
        "@types/react-dom": "^18.3.1",
        "@types/uuid": "^10.0.0",
        "@vitejs/plugin-react": "^4.3.3",
        "qr-scanner": "^1.4.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-icons": "^5.3.0",
        "react-qr-code": "^2.0.15",
        "react-scripts": "^5.0.1",
        "uuid": "^11.0.3"
    },
    "scripts": {
        "start": "vite",
        "build": "vite build",
        "lint": "eslint --fix --ext .js,.jsx,.tsx src",
        "check-ts": "tsc --noEmit --checkJS false",
        "tsc": "tsc --project tsconfig.json"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "homepage": ".",
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ],
        [
            "@babel/plugin-proposal-class-properties",
            {
                "loose": true
            }
        ]
    ]
}
